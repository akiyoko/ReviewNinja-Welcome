[project]
	description = Welcome repo for ReviewNinja to help you get started. Feel free to delete when you're done, we won't take it personally.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/akiyoko (akiyoko)
[access "refs/for/refs/*"]
	read = group user/akiyoko (akiyoko)
	push = group user/akiyoko (akiyoko)
	label-Code-Review = -2..+2 group user/akiyoko (akiyoko)
	label-Verified = -1..+1 group user/akiyoko (akiyoko)
	rebase = group user/akiyoko (akiyoko)
	removeReviewer = group user/akiyoko (akiyoko)
	submit = group user/akiyoko (akiyoko)
[access "refs/heads/*"]
	read = group user/akiyoko (akiyoko)
	create = group user/akiyoko (akiyoko)
	push = +force group user/akiyoko (akiyoko)
	pushMerge = group user/akiyoko (akiyoko)
[access "refs/tags/*"]
	push = +force group user/akiyoko (akiyoko)
	pushTag = group user/akiyoko (akiyoko)
	pushSignedTag = group user/akiyoko (akiyoko)
